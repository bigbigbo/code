{
  "name": "cool-feeling",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint:style": "stylelint \"src/**/*.{css,less,scss}\"",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint-staged:ts": "tslint -c tslint.json \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write ./src/**/**/**/*",
    "dev": "cross-env BABEL_ENV=development node webpack/dev-server.js",
    "build": "cross-env BABEL_ENV=production webpack --config webpack/webpack.prod.conf",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/node": "^8.0.11",
    "autoprefixer": "^9.1.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "8.0.0-beta.6",
    "babel-plugin-add-module-exports": "0.3.3",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-import": "^1.9.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "react-dev-utils": "^5.0.1",
    "react-hot-loader": "^4.3.4",
    "redbox-react": "^1.6.0",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "eslint",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "tslint",
      "git add"
    ],
    "**/*.{css,less,scss}": [
      "prettier --write",
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.9.2",
    "dva": "^2.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0"
  },
  "engines": {
    "node": ">= 6.11.5",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not ie <= 10"
  ]
}
